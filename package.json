{
  "name": "@uni-helper/uni-env",
  "type": "module",
  "version": "0.1.8",
  "packageManager": "pnpm@10.15.0",
  "description": "在 uni-app 中优雅地判断当前环境。",
  "author": "KeJun",
  "maintainers": [
    {
      "email": "wurui.dev@gmail.com",
      "name": "ModyQyW",
      "url": "https://modyqyw.top"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/uni-helper/uni-env#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-helper/uni-env.git"
  },
  "bugs": "https://github.com/uni-helper/uni-env/issues",
  "keywords": [
    "uni-app",
    "env"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "prepublishOnly": "pnpm build",
    "release": "bumpp",
    "start": "esno src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "std-env": "^3.9.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@types/node": "^22.18.0",
    "bumpp": "^10.2.3",
    "eslint": "^9.34.0",
    "esno": "^4.8.0",
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
